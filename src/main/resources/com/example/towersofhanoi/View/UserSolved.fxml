<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.towersofhanoi.Controller.UserSolvedController">
    <children>
        <Label fx:id="congratulationsLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="96.0" layoutY="43.0" prefHeight="100.0" prefWidth="830.0" style="-fx-background-color: #0FB4BB;" text="CONGRATULATIONS! YOU SOLVED IT!" textAlignment="CENTER" textFill="WHITE">
