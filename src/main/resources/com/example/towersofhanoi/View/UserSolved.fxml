      <Label fx:id="timeLabel" layoutX="96.0" layoutY="664.0" prefHeight="35.0" prefWidth="80.0" text="Time:">
         <font>
            <Font name="Mongolian Baiti" size="30.0" />
         </font>
      </Label>
      <Label fx:id="pointsLabel" layoutX="96.0" layoutY="734.0" prefHeight="35.0" prefWidth="91.0" text="Points:">
         <font>
            <Font name="Mongolian Baiti" size="30.0" />
