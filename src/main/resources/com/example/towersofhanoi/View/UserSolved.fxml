            <Font name="Mongolian Baiti" size="25.0" />
         </font>
      </Button>
      <Button fx:id="quitButton" layoutX="847.0" layoutY="880.0" mnemonicParsing="false" onAction="#quitButtonOnAction" style="-fx-background-color: #FA8163;" text="Quit" textAlignment="CENTER" textFill="WHITE">
